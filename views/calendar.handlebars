
 <div class="container-fluid">
    <div class="row col-12">
      <div class="card">
        <div class="card-body">
          You can make your appointments below
        </div>
      </div>
    </div>
  </br>
</div>

<div id='wrap'>
    <div id='pet-services'>
      <h4>Pet Grooming Services</h4>
      <div id='pet-services-list'>
            {{#each dataIn}}
                <div class='fc-event fc-h-event fc-daygrid-event fc-daygrid-block-event'>
                  <div class='fc-event-main'><h6>{{this.title}}</h6></div>
                  <div class="card" >
                    <div class="card-body">
                      <p class="card-text">{{this.description}}</p>
                    </div>
                  </div>
                </div>
            {{/each}}
      </div>
    </div>

    <div id='calendar-wrap'>
      <div id='calendar'></div>
    </div>

  </div>
<div></div>


	<script src="/js/myfullcalendar.js"></script>


